    "concurrently": "4.0.1",
    "core-js": "2.6.5",
    "glob": "7.1.1",
    "gulp": "3.9.1",
    "gulp-inline-ng2-template": "5.0.1",
    "jasmine-core": "2.5.2",
    "jointjs": "2.1.4",
    "jquery": "3.1.1",
    "jshint": "2.6.3",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-html-reporter": "0.2.7",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "lite-server": "2.4.0",
